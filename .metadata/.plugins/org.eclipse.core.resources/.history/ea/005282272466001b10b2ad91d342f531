package br.com.academia.model;

public class Pessoa {
	
	private String nome;
	private double peso;
	private double altura;
	private char sexo;
	private int nivelDeAtividade;

	
	//*set e get nome
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getNome() {
		return this.nome;
	}
	//*set e get peso
	public  void setPeso(double peso) {
		 this.peso = peso;
	}
	public double getPeso() {
		return this.peso;
	}
	



}

